###  urn:absolute:jgravier/cities_north_france.owl#aPourGeom
:aPourGeom rdf:type owl:ObjectProperty ;
           rdfs:label "a pour géométrie"@fr ,
                      "has geometry"@en .


###  urn:absolute:jgravier/cities_north_france.owl#estAccess
:estAccess rdf:type owl:ObjectProperty ;
           rdfs:domain :Troncon ;
           rdfs:range :Centre ,
                      :Pole ;
           rdfs:label "allows access"@en ,
                      "permet l'accès"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#estIdentifieSur
:estIdentifieSur rdf:type owl:ObjectProperty ;
                 rdfs:domain :Troncon ;
                 rdfs:range :cartesETMJ ;
                 rdfs:label "est identifié sur"@fr ,
                            "is identified on"@en .


###  urn:absolute:jgravier/cities_north_france.owl#estIncluDans
:estIncluDans rdf:type owl:ObjectProperty ;
              rdfs:domain :Pole ;
              rdfs:range :Centre ;
              rdfs:label "est inclu dans"@fr ,
                         "is included in"@en .


###  urn:absolute:jgravier/cities_north_france.owl#estInfereAPartirDe
:estInfereAPartirDe rdf:type owl:ObjectProperty ;
                    rdfs:domain :Centre ;
                    rdfs:range :cartesETMJ ;
                    rdfs:label "est inféré à partir de"@fr ,
                               "is inferred from"@en .


###  urn:absolute:jgravier/cities_north_france.owl#estLie
:estLie rdf:type owl:ObjectProperty ;
        rdfs:domain :Ville ;
        rdfs:range <http://rdf.insee.fr/def/geo#Commune> ;
        rdfs:label "est lié à"@fr ,
                   "is linked to"@en .


###  urn:absolute:jgravier/cities_north_france.owl#estReconstitueAvec
:estReconstitueAvec rdf:type owl:ObjectProperty ;
                    rdfs:domain :Pole ;
                    rdfs:range :AutreSource ;
                    rdfs:label "est reconstitué à partir de"@fr ,
                               "is reconstituted from"@en .


###  urn:absolute:jgravier/cities_north_france.owl#estTresDense
:estTresDense rdf:type owl:ObjectProperty ;
              rdfs:domain :Centre ;
              rdfs:range :Ville ;
              rdfs:comment "Espace urbain délimité par l’enceinte urbaine – quand elle est encore en élévation au moment des levés de l’état-major – ou l’espace anciennement délimité par l’enceinte urbaine. Renvoie généralement aux zones représentées sous forme d’aplats rouges sur les planches de cartes de l’état-major, relatifs à des îlots denses."@fr ,
                           "Urban space delimited by the urban enclosure – when it is still in elevation – or the space formerly delimited by the urban enclosure. Generally refers to the areas represented in the form of solid red blocks on the maps, relating to dense blocks"@en ;
              rdfs:label "dense city center"@en ,
                         "ville-centre dense"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#estUrbaniseContinu
:estUrbaniseContinu rdf:type owl:ObjectProperty ;
                    rdfs:domain :Centre ;
                    rdfs:range :Ville ;
                    rdfs:comment "Morphologie de l'espace urbain contenant la ville-centre et tissu d'habitats continus sans interuptions (limite d'environ 100 mètres)"@fr ,
                                 "Morphology of the urban space containing the city-centre and continuous habitats without interruptions (limit ~ 100 meters)"@en ;
                    rdfs:label "tissu urbain continu"@fr ,
                               "urban agglomeration"@en .


