#################################################################

###  http://data.ign.fr/def/geofla#coordX
geofla:coordX rdf:type owl:DatatypeProperty ;
              rdfs:domain :point ;
              rdfs:range xsd:double .


###  http://data.ign.fr/def/geofla#coordY
geofla:coordY rdf:type owl:DatatypeProperty ;
              rdfs:domain :point ;
              rdfs:range xsd:double .


###  http://data.ign.fr/def/geofla#numInsee
geofla:numInsee rdf:type owl:DatatypeProperty ;
                owl:equivalentProperty :insee .


###  http://data.ign.fr/def/geofla#superficieHa
geofla:superficieHa rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Centre ;
                    rdfs:range xsd:integer ;
                    rdfs:label "area in ha"@en .


###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdf:type owl:DatatypeProperty .


###  urn:absolute:jgravier/cities_north_france.owl#insee
:insee rdf:type owl:DatatypeProperty ;
       rdfs:domain <http://rdf.insee.fr/def/geo#Commune> .


###  urn:absolute:jgravier/cities_north_france.owl#longueur
:longueur rdf:type owl:DatatypeProperty ;
          rdfs:domain :Troncon ;
          rdfs:range xsd:float ;
          rdfs:label "length"@en .


#################################################################
#    Classes
#################################################################

###  http://rdf.insee.fr/def/geo#Commune
<http://rdf.insee.fr/def/geo#Commune> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://rdf.insee.fr/def/geo#Departement> ;
                                      rdfs:isDefinedBy <https://rdf.insee.fr/geo/index.html> ;
                                      rdfs:label "Commune"@fr ,
                                                 "Municipality"@en .


###  http://rdf.insee.fr/def/geo#Departement
<http://rdf.insee.fr/def/geo#Departement> rdf:type owl:Class ;
                                          rdfs:isDefinedBy <https://rdf.insee.fr/geo/index.html> ;
                                          rdfs:label "Departement"@fr ,
                                                     "Department"@en .


###  http://www.opengis.net/ont/sf#Line
<http://www.opengis.net/ont/sf#Line> rdf:type owl:Class ;
                                     owl:equivalentClass :polyligne ;
                                     rdfs:isDefinedBy <http://www.opengis.net/ont/sf> ,
                                                      "Track graph"@en .


###  http://www.opengis.net/ont/sf#MultiPolygon
<http://www.opengis.net/ont/sf#MultiPolygon> rdf:type owl:Class ;
                                             owl:equivalentClass :multipolygone ;
                                             rdfs:isDefinedBy <http://www.opengis.net/ont/sf> .


###  http://www.opengis.net/ont/sf#Point
<http://www.opengis.net/ont/sf#Point> rdf:type owl:Class ;
                                      owl:equivalentClass :point ;
                                      rdfs:isDefinedBy <http://www.opengis.net/ont/sf> .


###  urn:absolute:jgravier/cities_north_france.owl#AutreSource
:AutreSource rdf:type owl:Class ;
             rdfs:comment "Autres sources d'information : OSM, Google Map Satellite, cadastres napoléoniens (plans), livres d'histoire locale, etc. Pour chaque ville, des spécifications nécessitant des clarifications sont données avec la bibliographie"@fr ,
                          "Other sources of information: OSM, Google Map Satellite, Napoléonian cadastres (maps), local history books, etc. For each city, specifications requiring clarification are given with the bibliography"@en ;
             rdfs:label "autres sources"@fr ,
                        "other sources"@en .


###  urn:absolute:jgravier/cities_north_france.owl#Centre
:Centre rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :aPourGeom ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :multipolygone
                            ] ;
        rdfs:subClassOf :Ville ;
        rdfs:comment "Delimited area of the urban space"@en ,
                     "Zone délimitée de l'espace urbain"@fr ;
        rdfs:label "Urban area"@en ,
                   "Zone urbaine"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#Pole
:Pole rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :aPourGeom ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :point
                          ] ;
      rdfs:subClassOf <http://rdf.insee.fr/def/geo#Commune> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                        owl:hasValue <http://www.geonames.org/ontology#S.CH>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                        owl:hasValue <http://www.geonames.org/ontology#S.CSTL>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                        owl:hasValue <http://www.geonames.org/ontology#S.HSTS>
                      ] ;
      rdfs:comment "Historical institutional pole materialized by an establishment in the urban space"@en ,
                   "Pôle institutionnel historique matérialisé par un établissement dans l'espace urbain"@fr ;
      rdfs:label "Historical urban center"@en ,
                 "Pôle urbain historique"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#Troncon
:Troncon rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :aPourGeom ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :polyligne
                             ] ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                           owl:hasValue <http://www.geonames.org/ontology#R.RD>
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                           owl:hasValue <http://www.geonames.org/ontology#R.RDJCT>
                         ] ;
         rdfs:comment "A section of road, street, or path as depicted on état-major maps that provides access to infra-urban spaces and the city with other settlements. Rivers which are navigable are not considered."@en ,
                      "Tronçon de route, de rue ou de chemin représenté sur les cartes d'état-major permettant l'accessibilité des espaces infra-urbains et de la ville avec d'autres peuplements. Ne sont pas considérés les rivières navigables."@fr ;
         rdfs:label "Section"@en ,
                    "Tronçon"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#Ville
:Ville rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :aPourGeom ;
                             owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                             owl:onClass :multipolygone
                           ] ;
       rdfs:subClassOf <http://rdf.insee.fr/def/geo#Departement> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.geonames.org/ontology#featureCode> ;
                         owl:hasValue <http://www.geonames.org/ontology#S.HSTS>
                       ] ;
       rdfs:comment "\"Ville\" or \"Bourg\" according to the typography used on état-major maps to designate these settlements"@en ,
                    "\"Ville\" ou \"Bourg\" d'après les casses typographiques utilisées sur les cartes d'état-major pour désigner ces peuplements"@fr ;
       rdfs:label "City"@en ,
                  "Ville"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#cartesETMJ
:cartesETMJ rdf:type owl:Class ;
            rdfs:comment "IGN SCAN État-major® : cartes d’état-major au 1:40 000 sur la France métropolitaine"@fr ,
                         "IGN SCAN État-major®: 1:40,000-scale état-major maps of French territory"@en ;
            rdfs:label "cartes d'état-major"@fr ,
                       "état-major maps"@en .


###  urn:absolute:jgravier/cities_north_france.owl#multipolygone
:multipolygone rdf:type owl:Class ;
               rdfs:label "Multipolygon"@en ,
                          "Multipolygone"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#point
:point rdf:type owl:Class ;
       rdfs:label "Point"@en ,
                  "Point"@fr .


###  urn:absolute:jgravier/cities_north_france.owl#polyligne
:polyligne rdf:type owl:Class ;
           rdfs:label "Ligne"@fr ,
                      "Line"@en .


